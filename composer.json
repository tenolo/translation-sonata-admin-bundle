{
    "name": "tenolo/translation-sonata-admin-bundle",
    "description": "Implements translation admin functions.",
    "keywords": [
        "translation",
        "tenolo",
        "admin",
        "symfony",
        "sonata",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/form": "~2.8|~3.0|~4.0",
        "symfony/yaml": "~2.8|~3.0|~4.0",

        "sonata-project/admin-bundle": "^3.45",
        "tenolo/translation-bundle": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\TranslationAdminBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
